<%= link_to "Models Index", models_path %>
<%= link_to "New Model", newgear_path %>


<h1>Instruments</h1>
<% sorted = @models.sort_by { |model| model[:name] } %>
<% sorted.each do |model|  %>
  <h2>Name:<%= model.name  %></h2>
  <p>Year: <%= model.year %></p>
  <p>Is it vintage?:<%= model.vintage %></p>
  <p>What is it worth?:<%= model.value %></p>
<% end %>