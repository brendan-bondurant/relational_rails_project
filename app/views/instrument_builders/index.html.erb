<%= link_to "New Builder", "/instrument_builders/new" %>

<p><h1>All Builders</h1></p>
<% builders = @instrument_builders.order(created_at: :asc) %>

<% builders.each do |builder| %>
  <h2>Builder:<%= builder.name %> <%= button_to "Edit #{builder.name}", "/instrument_builders/#{builder.id}/edit", method: :get%></h2>
  <p>Created at: <%= builder.created_at %></p>
  <% end %>
